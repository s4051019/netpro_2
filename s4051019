private javax.swing.JTextField jTextField2;
private javax.swing.JTextPane jTextPane1;
private javax.swing.JTextPane jTextPane2;
// End of variables declaration

public class threadInit extends Thread {

String threadName, address_;
JEditorPane pane1, pane2;
thread x;
String show_header;

threadInit(String name, String address, JEditorPane pane1_, JEditorPane pane2_) {
threadName = name;
address_ = address;
pane1 = pane1_;
pane2 = pane2_;
}

@Override
public void run() {
x = new thread();
try {
if (threadName.equals("Thread1")) {
JOptionPane.showMessageDialog(jEditorPane1, "Thread1");
pane1.setPage(address_);
URL url1 = new URL(address_);
URLConnection conn = url1.openConnection();
Map<String, List<String>> map = conn.getHeaderFields();
show_header = threadName+" Start!!!\n";
show_header += "[URL]" + address_+"\n";
show_header += "Printing Response Header...\n";

for (Map.Entry<String, List<String>> entry : map.entrySet()) {
show_header += entry.getKey() + ":" + entry.getValue() + "\n";
}

show_header += "\nGet Response Header By Key ...\n";
String server = conn.getHeaderField("Server");

if (server == null) {
show_header += "Key 'Server' is not found!";
} else {
show_header += "Server - " + server;
}
show_header += "\n Done\n";
show_header += threadName+" Stop!!!\n";
jTextPane1.setText(show_header);
} else {
JOptionPane.showMessageDialog(jEditorPane2, "Thread2");
pane2.setPage(address_);
URL url1 = new URL(address_);
URLConnection conn = url1.openConnection();
Map<String, List<String>> map = conn.getHeaderFields();
show_header = threadName+" Start!!!\n";
show_header += "[URL]" +address_+"\n";
show_header += "Printing Response Header...\n";

for (Map.Entry<String, List<String>> entry : map.entrySet()) {
show_header += entry.getKey() + ":" + entry.getValue() + "\n";
}

show_header += "\nGet Response Header By Key ...\n";
String server = conn.getHeaderField("Server");

if (server == null) {
show_header += "Key 'Server' is not found!";
} else {
show_header += "Server - " + server;
}
show_header += "\n Done\n";
show_header += threadName+" Stop!!!\n";
jTextPane2.setText(show_header);
}
}
catch (IOException ex


) {
Logger.getLogger(threadInit.class.getName()).log(Level.SEVERE, null, ex);
}
}

}
}
